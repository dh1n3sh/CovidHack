    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.arumugam_activities.MapsModule" />

    <include layout="@layout/topbarmapslayoutarumugam"/>
    <include layout="@layout/bottom_sheet_hospital" android:visibility="gone"/>
    <include layout="@layout/bottom_sheet_labs" android:visibility="gone"/>
    <include layout="@layout/bottom_sheet_shelter" android:visibility="gone"/>
</androidx.coordinatorlayout.widget.CoordinatorLayout>